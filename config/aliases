# Get External IP / Internet Speed
alias myip="curl https://ipinfo.io/json" # or /ip for plain-text ip
alias speedtest="curl -s https://raw.githubusercontent.com/sivel/speedtest-cli/master/speedtest.py | python -"



# in X3PI ROS2
# alias tros="source /opt/ros/foxy/setup.zsh && source /opt/tros/setup.zsh"

# reboot win 
alias rebootwin="sudo bash /home/robot/rebootwin.sh"

# wechat
alias wechat="/home/robot/.fishros/bin/wechat"


# sudo apt update
alias sau="sudo apt update"
alias sauu="sudo apt update"
alias sai="sudo apt install"
alias sar="sudo apt remove"
alias sdi="sudo dpkg -i"
alias sdr="sudo dpkg -r"


